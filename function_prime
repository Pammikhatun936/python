def is_prime(num):
    count = 0
    for i in range(1, num + 1):
        if num % i == 0:
            count += 1
    if count == 2:
        return True
    else:
        return False


def sum_of_primes(n):
    total = 0
    for i in range(2, n + 1):
        if is_prime(i):     
            total += i
    return total



n = int(input("Enter a number: "))
print("Sum of prime numbers up to", n, "is:", sum_of_primes(n))
